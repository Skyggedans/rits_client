<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config>
        <domain includeSubdomains="true">rockwellits.com</domain>
        <trust-anchors>
            <certificates src="@raw/rits_ca"/>
        </trust-anchors>
    </domain-config>
    <domain-config>
        <domain includeSubdomains="true">commondatastorage.googleapis.com</domain>
        <trust-anchors>
            <certificates src="@raw/storage_ca"/>
        </trust-anchors>
    </domain-config>
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system"/>
            <certificates src="user"/>
        </trust-anchors>
    </base-config>
    <debug-overrides>
        <trust-anchors>
            <certificates src="user"/>
        </trust-anchors>
    </debug-overrides>
</network-security-config>